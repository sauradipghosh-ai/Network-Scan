generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Scan{
  id Int @id @default(autoincrement())
  ip String
  hostname String?
  status String
  scanDate DateTime
  ports Port[]

  @@map("scans")
  
}

model Port{
  id Int @id @default(autoincrement())
  port Int
  protocol String
  state String
  service String
  version String?
  scanId Int
  scan Scan @relation(fields: [scanId],references: [id])
}
